function Populate(inForm)
{
  var someoptions = new Array(systemlist.length);
  
  for (var i = 0;i < systemlist.length;i++) {
    someoptions[i] = new Option(systemlist[i][1], systemlist[i][2]);
    eval("inForm.Model.options[i]=someoptions[" + i + "]");
    if (i == 0) {
      inForm.Model.options[i].selected=true;
    }
  }
}


function Refill(listbox,optiontype,code)
{
  var i;
  var j = 1;
  var someoptions = new Array(10);

  for (i=1;i < listbox.options.length;i++) {
    listbox.options[i] = null;
  }
  listbox.options[0].selected=true;
  
  for (i = 0;i < optionlist.length;i++) {
    if (optionlist[i][0] == optiontype) {
      if (optionlist[i][1].indexOf(code.substring(0,6)) != -1) {
        someoptions[j] = new Option(optionlist[i][2],optionlist[i][3]);
        eval("listbox.options[" + j + "]=someoptions[" + j + "]");
        j = j + 1;
      }
    }
  }
}

function FlowChanges(inForm)
{
  var system = inForm.Model.selectedIndex;
  
  inForm.processor.value=systemlist[system][0];
  inForm.OS.value=systemlist[system][7];
  inForm.casetype.value=systemlist[system][9];
  inForm.cache.value=systemlist[system][5];
  inForm.memory.value=systemlist[system][4];
  inForm.disk.value=systemlist[system][3];
  inForm.videomemory.value=systemlist[system][6];
  
  system = systemlist[system][2];
  
  Refill(inForm.plusmemory,"1",system)
  Refill(inForm.plusdisk,"5",system)
  Refill(inForm.plusvideomemory,"4",system)
  
  inForm.Model.focus();
}

